{
  "root": true,
  "ignorePatterns": ["node_modules/**/*", "dist/**/*"],
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:@typescript-eslint/stylistic"
  ],
  "rules": {
    "no-console": "error",
    "@typescript-eslint/no-unused-vars": "error"
  }
}
