{
  "name": "@juxio/react-styled",
  "version": "0.5.7",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "tokens",
    "processors",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jux-io/toolkit.git",
    "directory": "packages/react/styled"
  },
  "homepage": "https://github.com/jux-io/toolkit",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./tokens": {
      "types": "./tokens/index.d.ts",
      "import": "./tokens/index.mjs",
      "require": "./tokens/index.js",
      "default": "./tokens/index.js"
    }
  },
  "wyw-in-js": {
    "tags": {
      "css": "./processors/css.js",
      "styled": "./processors/styled.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "pnpm build --watch src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@wyw-in-js/processor-utils": "^0.5.4",
    "@wyw-in-js/shared": "^0.5.4",
    "@wyw-in-js/transform": "^0.5.4",
    "@juxio/core": "workspace:*",
    "csstype": "^3.1.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/types": "^7.25.2",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.2.18",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  }
}
