{
  "name": "@juxio/postcss",
  "description": "PostCSS plugin for Jux",
  "version": "0.8.5",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "jux",
    "react",
    "css-in-js",
    "css",
    "styled"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jux-io/toolkit.git",
    "directory": "packages/postcss"
  },
  "homepage": "https://github.com/jux-io/toolkit",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "pnpm build --watch src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@juxio/core": "workspace:*",
    "fs-extra": "^11.2.0",
    "postcss": "^8.4.39"
  },
  "publishConfig": {
    "access": "public"
  }
}
