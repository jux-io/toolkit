{
  "name": "@juxio/css",
  "version": "0.6.14",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "tokens",
    "processors",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jux-io/toolkit.git",
    "directory": "packages/css"
  },
  "homepage": "https://github.com/jux-io/toolkit",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./tokens": {
      "types": "./tokens/index.d.ts",
      "require": "./tokens/index.js",
      "import": "./tokens/index.mjs"
    },
    "./processor": {
      "require": "./processors/css.js",
      "default": "./processors/css.js"
    }
  },
  "wyw-in-js": {
    "tags": {
      "css": "./processors/css.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "pnpm build --watch src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@juxio/core": "workspace:*",
    "@wyw-in-js/processor-utils": "^0.5.4",
    "@wyw-in-js/shared": "^0.5.4",
    "@wyw-in-js/transform": "^0.5.4",
    "crypto-js": "^4.2.0",
    "csstype": "^3.1.3",
    "lodash": "^4.17.21",
    "type-fest": "^4.26.0"
  },
  "devDependencies": {
    "@babel/types": "^7.25.2",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.7"
  }
}
